	public:
		int roleid;
		unsigned int localsid;
		enum { PROTOCOL_TYPE = PROTOCOL_FACTIONLISTONLINE };
	public:
		FactionListOnline() { type = PROTOCOL_FACTIONLISTONLINE; }
		FactionListOnline(void*) : Protocol(PROTOCOL_FACTIONLISTONLINE) { }
		FactionListOnline (int l_roleid,unsigned int l_localsid)
			 : roleid(l_roleid),localsid(l_localsid)
		{
			type = PROTOCOL_FACTIONLISTONLINE;
		}

		FactionListOnline(const FactionListOnline &rhs)
			: Protocol(rhs),roleid(rhs.roleid),localsid(rhs.localsid) { }

		GNET::Protocol *Clone() const { return new FactionListOnline(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			os << localsid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			os >> localsid;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 32; }
